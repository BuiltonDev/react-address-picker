{
  "presets": [
    ["@babel/preset-react"],
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [ "safari 10", "ie 11", "ios 9" ]
        },
        "modules": false,
        "useBuiltIns": "usage",
        "loose": true
      }
    ]
  ],
  "plugins": ["@babel/plugin-proposal-object-rest-spread", "react-css-modules"],
  "comments": false,
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env"],
        ["@babel/preset-react"]
      ]
    }
  }
}
